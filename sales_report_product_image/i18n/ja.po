#. module: sales_report_product_image
#: model:ir.ui.view,arch_db:sales_report_product_image.report_saleorder_document_inherit
msgid "<?xml version=\"1.0\"?>\n"
"<data inherit_id=\"sale.report_saleorder_document\">\n"
"         <xpath expr=\"//table[@class='table table-condensed']/thead/tr/th[1]\" position=\"before\">\n"
"             <t t-if=\"doc.print_image and doc.image_sizes in ('image', 'image_medium', 'image_small') \">\n"
"                 <th>\n"
"                    Product Image\n"
"                </th>\n"
"             </t>\n"
"        </xpath>\n"
"        <xpath expr=\"//table/t/t[2]/tr/td[1]\" position=\"before\">\n"
"            <t t-if=\"doc.image_sizes == 'image' and doc.print_image \">\n"
"                <td>\n"
"                    <span t-field=\"l.product_id.image\" t-field-options=\"{&quot;widget&quot;: &quot;image&quot;, &quot;class&quot;: &quot;img-rounded&quot;}\"/>\n"
"                </td>\n"
"            </t>\n"
"            <t t-if=\"doc.image_sizes == 'image_medium' and doc.print_image\">\n"
"                <td>\n"
"                    <span t-field=\"l.product_id.image_medium\" t-field-options=\"{&quot;widget&quot;: &quot;image&quot;, &quot;class&quot;: &quot;img-rounded&quot;}\"/>\n"
"                </td>\n"
"            </t>\n"
"            <t t-if=\"doc.image_sizes == 'image_small' and doc.print_image \">\n"
"                <td>\n"
"                    <span t-field=\"l.product_id.image_small\" t-field-options=\"{&quot;widget&quot;: &quot;image&quot;, &quot;class&quot;: &quot;img-rounded&quot;}\"/>\n"
"                </td>\n"
"            </t>\n"
"        </xpath>\n"
"    </data>\n"
"\n"
""
msgstr ""

#. module: sales_report_product_image
