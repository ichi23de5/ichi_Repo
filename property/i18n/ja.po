#. module: property
#: model:ir.ui.view,arch_db:property.inspection_view_form
msgid "<?xml version=\"1.0\"?>\n"
"<form string=\"Inspection\">\n"
"                    <sheet>\n"
"                        <group>\n"
"                            <group>\n"
"		    	        <field name=\"date\"/>\n"
"                                <field name=\"inspector_id\" widget=\"selection\"/>\n"
