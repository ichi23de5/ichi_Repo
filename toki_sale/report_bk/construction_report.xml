<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

    <!-- toki construction -->
    <record id='report_sale_construction_odt' model='ir.actions.report.xml'>
        <field name='report_type'>aeroo</field>
        <field name='in_format'>oo-odt</field>
        <field name='out_format' eval="ref('report_aeroo.report_mimetypes_odt_odt')"/>
        <field name='multi' eval='False'/>
        <field name='name'>Syanai Construction</field>
        <field name='report_rml'>toki_sale/report/sale_construction.odt</field>
        <field name='report_name'>sale_construction_odt</field>
        <field name='model'>sale.order</field>
        <field name='type'>ir.actions.report.xml</field>
        <field name='attachment_use' eval='False'/>
        <field name='parser_loc'>toki_sale/report/parser.py</field>
        <field name='tml_source'>file</field>
        <field name='parser_state'>loc</field>
        <field name='styles_mode'>default</field>
    </record>

    <ir_set>
        <field eval="'action'" name="key"/>
        <field eval="'client_print_multi'" name="key2"/>
        <field eval="['sale.order']" name="models"/>
        <field name="name">Syuanai construction</field>
        <field eval="'ir.actions.report.xml,'+str(report_sale_construction_odt)" name="value"/>
        <field eval="True" name="isobject"/>
        <field eval="True" name="replace"/>
    </ir_set>

    </data>
</openerp>

